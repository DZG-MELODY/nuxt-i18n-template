{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nuxt: Client",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "dev"
      ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Nuxt: Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack://_N_E/*": "${webRoot}/*",
        "webpack:///./*": "${webRoot}/*",
        "webpack:///./~/*": "${workRoot}/node_modules/*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Nuxt: Server",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "dev"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std",
      "serverReadyAction": {
        "pattern": "Local:.+(https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    }
  ],
  "compounds": [
    {
      "name": "Nuxt: Full Stack",
      "configurations": ["Nuxt: Server", "Nuxt: Chrome"],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Nuxt",
        "order": 1
      }
    }
  ]
}
